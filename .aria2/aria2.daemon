# Basic download settings
continue
daemon=true
file-allocation=falloc
log-level=warn
max-connection-per-server=16
max-concurrent-downloads=3
max-overall-download-limit=0
min-split-size=2M
enable-http-pipelining=true

enable-rpc=true
rpc-listen-all=true

# Proxy
http-proxy=127.0.0.1:20171
https-proxy=127.0.0.1:20171
ftp-proxy=127.0.0.1:20171
